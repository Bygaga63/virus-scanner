{
  "name": "nodejs-rest-client",
  "version": "0.0.1",
  "description": "Rest client for virus scanner",
  "author": "Pavel Varentsov",
  "license": "MIT",
  "scripts": {
    "install:all": "./scripts/yarn-install.sh",
    "build": "./scripts/compiler.sh",
    "copy:env": "cp ./env/.env ./dist/",
    "start": "node ./dist/bootstrap.js",
    "start:env": "yarn copy:env && cd ./dist && node -r dotenv/config . dotenv_config_path=.env bootstrap.js",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "test:unit": "./node_modules/.bin/jest --config jest-unit.json",
    "test:unit:coverage": "./node_modules/.bin/jest --coverage --config jest-unit.json"
  },
  "engines": {
    "node": ">=11"
  },
  "engineStrict": true,
  "devDependencies": {
    "@nestjs/common": "6.10.11",
    "@nestjs/core": "6.10.11",
    "@nestjs/testing": "6.10.11",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.17",
    "dotenv": "8.2.0",
    "jest": "24.9.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  }
}
